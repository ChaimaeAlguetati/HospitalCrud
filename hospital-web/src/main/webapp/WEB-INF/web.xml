<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<servlet>
		<servlet-name>patient</servlet-name>
		<servlet-class>com.hospital.is.PatientServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>CreatePatient</servlet-name>
		<display-name>CreatePatient</display-name>
		<description></description>
		<servlet-class>com.hospital.is.CreatePatient</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>UpdatePatient</servlet-name>
		<display-name>UpdatePatient</display-name>
		<description></description>
		<servlet-class>com.hospital.is.UpdatePatient</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>deletePatient</servlet-name>
		<display-name>deletePatient</display-name>
		<description></description>
		<servlet-class>com.hospital.is.deletePatient</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>patient</servlet-name>
		<url-pattern>/patient</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>CreatePatient</servlet-name>
		<url-pattern>/CreatePatient</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UpdatePatient</servlet-name>
		<url-pattern>/UpdatePatient/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>deletePatient</servlet-name>
		<url-pattern>/deletePatient</url-pattern>
	</servlet-mapping>
</web-app>
